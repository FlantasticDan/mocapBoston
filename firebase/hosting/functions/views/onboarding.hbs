                    </div>
                </div>

                <!-- Session ID Segment Sample -->
                <div class="ui segment" style="width: fit-content; margin: auto; margin-top: 10px;">
                    <div class="ui mini statistic">
                        <div class="value">
                            {{id}}
                        </div>
                        <div class="label">
                            Session ID
                        </div>
                    </div>
                </div>

            </div>
        </div>


        <!-- The core Firebase JS SDK is always required and must be listed first -->
